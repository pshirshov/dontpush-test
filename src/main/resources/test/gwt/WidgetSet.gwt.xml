<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module>
    <!-- This file is automatically updated based on new dependencies by the
         goal "vaadin:update-widgetset". -->

    <!-- Inherit DefaultWidgetSet -->
    <inherits name="com.vaadin.terminal.gwt.DefaultWidgetSet" />

    <!-- Our example uses google map as base layer. If you are e.g drawing base layer from
         your own map server (WMS or TMS), you don't need these. -->
    <script src="http://maps.google.com/maps/api/js?v=3.2&amp;sensor=false&amp;language=ru" />

    <!-- Inherit Widgetset which contains vaadin wrappers and also injects open
         layers scripts from openlayers.org. Alternatively you can use just VolWidgetset
         if you wish to host the open layers scripts by yourself. -->
    <inherits name="org.vaadin.vol.VolWidgetsetWithHostedScript" />

    <inherits name="org.vaadin.dontpush.widgetset.DontPushOzoneWidgetset" />
</module>
